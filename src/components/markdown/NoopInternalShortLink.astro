---
import type { Props } from './InternalShortLink.astro';

const props: Props = Astro.props;
---

<pre>&lt;InternalShortLink identifier&#x3D;&quot;{props.identifier}&quot;{!Astro.slots.has('default') ? <>&#x2F;&gt;</> : <>&gt;</>}</pre>
{
  Astro.slots.has('default') && (
    <>
      <slot />
      <pre>&lt;&#x2F;InternalShortLink &gt;</pre>
    </>
  )
}
