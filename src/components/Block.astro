---
import { getCollection } from 'astro:content';
interface Props {
  identifier: string;
  lang: string;
}

const { identifier, lang } = Astro.props;

const blockEntries = await getCollection('blocks', (block) => {
  return block.slug === `${identifier}/${lang}`;
});
console.log(identifier, lang);
const { Content } = await blockEntries[0].render();
---

<Content />
