---
import { type LanguagesValue } from '../model/languages';

import BaseLayout from './BaseLayout.astro';

interface Props {
  title: string;
  lang: LanguagesValue;
}

const { lang, title } = Astro.props;

import './theme-print.css';
---

<BaseLayout
  title={title}
  lang={lang}
  seo=""
  format={Astro.locals.isFlyer ? 'a6' : 'a4'}
>
  <slot />
</BaseLayout>
