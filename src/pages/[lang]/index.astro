---
import { type LanguagesValue, SupportedLanguages } from '../../model/languages';

import LangingPage from '../../components/LangingPage.astro';
import { mapStaticPathsForPaths } from '../../util/map-static-paths-for-paths';

interface Props {
  lang: LanguagesValue;
}

export async function getStaticPaths() {
  return mapStaticPathsForPaths([...SupportedLanguages]);
}

const { lang } = Astro.props;
---

<LangingPage lang={lang} />
