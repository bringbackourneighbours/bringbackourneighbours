---
import { type LanguagesValue } from '../../model/languages';

import { getStaticPathsForPaths } from '../../util/get-static-paths-for-standalone';

import LangingPage from '../../components/LangingPage.astro';

interface Props {
  lang: LanguagesValue;
}

export async function getStaticPaths() {
  return getStaticPathsForPaths();
}

const { lang } = Astro.props;
---

<LangingPage lang={lang} />
