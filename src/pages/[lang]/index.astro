---
import { SupportedLanguages, type LanguagesValue } from '../../util/languages';

import LangingPage from '../../components/LangingPage.astro';

interface Props {
  lang: LanguagesValue;
}

export async function getStaticPaths() {
  return Object.values(SupportedLanguages).map((lang) => {
    return {
      params: { lang },
      props: { lang },
    };
  });
}

const { lang } = Astro.props;
---

<LangingPage lang={lang} />
